<div class="container-fluid e_page skilling-path" id="skilling_path" style="display:none;">
    <div class="container-fluid">
        <div class="row">
            <div class="col-11">
                <div class="mt-5">
                    <h3 class="breadcrumb-title text-dark">My learning & event library</h3>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 mt-4" style="background: #d1d5db; padding: 40px 30px 20px 30px;">
            <div class="row e_skill_items items">
                <% var skilling_data = [
                    {"img": "marketpace-uiux.png", "tag": "Udemy", "title": "User experience design fundamentals", "author": "Joe Natoli", "students": "87,353"},
                    {"img": "marketplace-ux.jpg", "tag": "Udemy", "title": "Sketching for ux designers - boost ux work with pen & paper!", "author": "Kristina Szerorny", "students": "18,547"},
                    {"img": "marketplace-powerbi.png", "tag": "Skillshare", "title": "Microsoft Power BI - complete beginners guide to financial dashboards", "author": "Charlie Walker", "students": "4,353"},
                ]; %>

                <% skilling_data.forEach(function(e, index){ %>
                    <div class="col-md-3 mb-4 e_item" index="<%-index%>" draggable="true" ondragstart="drag(event, `<%-e.img%>`, `<%-e.title%>`)">
                        <div class="card h-100">
                            <div class="position-relative card-div-top">
                                <img src="img/<%-e.img%>" class="card-img-top">
                                <button class="btn btn-secondary" function="remove"><i class="fa fa-times"></i> Remove</button>
                                <span class="text-uppercase <%-e.tag.toLowerCase()%>"><%-e.tag%></span>
                            </div>

                            <div class="card-body">
                                <h6 class="card-title text-capitalize text-dark mb-0"><%-e.title%></h6>
                                <small class="text-muted">by <%-e.author%></small>
                            </div>
                            <div class="card-footer border-0">
                                <div class="float-left">
                                    <img src="img/marketplace-rating.png">
                                </div>
                                <small class="float-right text-muted"><%-e.students%> students</small>
                            </div>
                        </div>
                    </div>
                <% }) %>

                <div class="col-md-3 mb-4 e_item hackathon" index="999" draggable="true" ondragstart="drag(event, `hackathons-mobilityapp.jpg`, `Creating a new mobility app`)">
                    <div class="card h-100">
                        <div class="position-relative card-div-top">
                            <div>
                                <img src="img/hackathons-mobilityapp.jpg" class="card-img-top">
                            </div>
                            <button class="btn btn-secondary" function="remove"><i class="fa fa-times"></i> Remove</button>
                            <h4 class="card-title text-capitalize">Creating a new mobility app</h4>
                        </div>

                        <div class="card-body">
                            <div class="event-info">
                                <table>
                                    <tr>
                                        <th width="50%">Start Date</th>
                                        <th width="30%">Duration</th>
                                        <th width="20%">Location</th>
                                    </tr>
                                    <tr>
                                        <td>17 Jan 2022</td>
                                        <td>7 Days</td>
                                        <td>Remote</td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                        <div class="card-footer border-0">
                            <div class="float-left">
                                <img src="img/skilling-hackathon.png">
                            </div>
                            <a href="javascript:void(0)" class="btn btn-light border-0 float-right" disabled="true">Joined</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-12" style="height: 1050px;">
            <div class="text-center mt-4">
                <h2 class="text-dark">Your individual skilling path</h2>
                <h5 class="text-dark mt-3"><b>Drag and drop the items into your skilling path</b></h5>
            </div>
            <div class="items skill-paths text-center mt-5 pt-3">
                <div class="row" style="justify-content: center; margin-top: 102px;">
                    <div class="col-md-8 path-border-1">
                        <div class="row path-border-1-items">
                            <div class="col-4">
                                <div class="skill-path position-relative" ondrop="drop(event)" ondragover="allowDrop(event)">
                                    <div class="skill-attachment">
                                        <i class="far fa-file"></i>
                                    </div>
                                    <div class="e_item_slot no-item">
                                        <span class="icon">
                                            <i class="fa fa-plus"></i>
                                        </span>
                                        <p class="mt-3 text-muted">Add learning topic</p>
                                    </div>
                                </div>
                            </div>
                            <div class="col-4">
                                <div class="skill-path position-relative" ondrop="drop(event)" ondragover="allowDrop(event)">
                                    <div class="skill-attachment">
                                        <i class="far fa-file"></i>
                                    </div>
                                    <div class="e_item_slot no-item">
                                        <span class="icon">
                                            <i class="fa fa-plus"></i>
                                        </span>
                                        <p class="mt-3 text-muted">Add learning topic</p>
                                    </div>
                                </div>
                            </div>
                            <div class="col-4">
                                <div class="skill-path position-relative" ondrop="drop(event)" ondragover="allowDrop(event)">
                                    <div class="skill-attachment">
                                        <i class="far fa-file"></i>
                                    </div>
                                    <div class="e_item_slot no-item">
                                        <span class="icon">
                                            <i class="fa fa-plus"></i>
                                        </span>
                                        <p class="mt-3 text-muted">Add learning topic</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-8 path-border-2">
                        <div class="row path-border-2-items">
                            <div class="col-6">
                                <div class="skill-path position-relative" ondrop="drop(event)" ondragover="allowDrop(event)">
                                    <div class="skill-attachment calendar">
                                        <i class="far fa-calendar"></i>
                                    </div>
                                    <div class="e_item_slot no-item">
                                        <span class="icon">
                                            <i class="fa fa-plus"></i>
                                        </span>
                                        <p class="mt-3 text-muted">Add Hackathon event</p>
                                    </div>
                                </div>
                            </div>
                            <div class="col-6">
                                <div class="skill-path position-relative" ondrop="drop(event)" ondragover="allowDrop(event)">
                                    <div class="skill-attachment">
                                        <i class="far fa-file"></i>
                                    </div>
                                    <div class="e_item_slot no-item">
                                        <span class="icon">
                                            <i class="fa fa-plus"></i>
                                        </span>
                                        <p class="mt-3 text-muted">Add learning topic</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row path-border-3-items">
                            <div class="col-12">
                                <div class="skill-path position-relative" >
                                    <div class="skill-attachment camera">
                                        <i class="fa fa-camera"></i>
                                    </div>
                                    <input type="file" accept="image/*" name="upload_video" id="upload_video"  onchange="loadFile(event)" style="display: none;">
                                    <div class="e_item_slot no-item e_upload_video">
                                        <span class="icon">
                                            <i class="fa fa-upload"></i>
                                        </span>
                                        <p class="mt-3 text-muted">Upload your video</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>